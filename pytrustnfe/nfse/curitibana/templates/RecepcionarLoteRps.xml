<EnviarLoteRpsEnvio xmlns = "http://isscuritiba.curitiba.pr.gov.br/iss/nfse.xsd" xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation = "http://isscuritiba.curitiba.pr.gov.br/iss/nfse.xsd">
    <LoteRps>
        <NumeroLote>{{ nfse.numero_lote }}</NumeroLote>
        <Cnpj>{{ nfse.cnpj_prestador }}</Cnpj>
        <InscricaoMunicipal>{{ nfse.inscricao_municipal }}</InscricaoMunicipal>
        <QuantidadeRps>{{ nfse.lista_rps|length }}</QuantidadeRps>
        <ListaRps>
            {% for rps in nfse.lista_rps -%}
            {% include 'Rps.xml' %}
            {% endfor %}
        </ListaRps>
    </LoteRps>
</EnviarLoteRpsEnvio>
